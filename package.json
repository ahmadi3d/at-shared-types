{
  "name": "at-shared-types",
  "version": "1.2.7",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./domain": {
      "types": "./dist/domain.d.ts",
      "require": "./dist/domain.cjs",
      "import": "./dist/domain.mjs"
    },
    "./transport": {
      "types": "./dist/transport.d.ts",
      "require": "./dist/transport.cjs",
      "import": "./dist/transport.mjs"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "require": "./dist/protocol.cjs",
      "import": "./dist/protocol.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup && tsc -p tsconfig.types.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}