{
  "name": "at-shared-types",
  "version": "1.1.26",
  "type": "commonjs",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && node scripts/postbuild.mjs"
  },
  "devDependencies": {
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  }
}