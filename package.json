{
  "name": "at-shared-types",
  "version": "1.1.31",
  "description": "Shared domain + transport DTO types for AT backend and frontend.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts --sourcemap --target es2020 --out-dir dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
